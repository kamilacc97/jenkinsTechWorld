pipeline{
  
  agent any
  
  stages{
  
    stage("build"){
      steps{
        echo 'build the app...'
         echo 'Application build'
      }
    }
    
    stage("tets"){
      steps{
        echo 'test the app...'
      }
    }
    
    stage("deploy"){
      steps{
        echo 'deploying the app...'
      }
    }
    
  }
}
